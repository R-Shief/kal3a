{
    "name": "rshief/kal3a",
    "license": "MIT",
    "type": "project",
    "description": "Aggregation and collection engine.",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ],
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {
        "php": ">=7.0.0",
        "anahkiasen/underscore-php": "^2.0",
        "aws/aws-sdk-php-symfony": "^1.2",
        "bangpound/atom-models": "^2.0",
        "bangpound/callable-compiler-pass": "^1.0",
        "bangpound/guzzle-proxy-bundle": "^2.0",
        "doctrine/couchdb": "dev-develop as 1.0.x-dev",
        "doctrine/couchdb-odm": "dev-develop as 1.0.x-dev",
        "doctrine/couchdb-odm-bundle": "dev-develop",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/jsrouting-bundle": "dev-feature/non-standard-port as 2.0.x-dev",
        "friendsofsymfony/rest-bundle": "^2.0",
        "friendsofsymfony/user-bundle": "^2.0",
        "incenteev/composer-parameter-handler": "^2.0",
        "javiereguiluz/easyadmin-bundle": "^1.13",
        "league/csv": "^8.1",
        "liip/monitor-bundle": "^2.3",
        "nelmio/api-doc-bundle": "^2.11",
        "nelmio/cors-bundle": "^1.0",
        "ongr/elasticsearch-bundle": "5.1.x-dev",
        "php-amqplib/rabbitmq-bundle": "^1.10",
        "phpdocumentor/reflection": "^2.0",
        "r-shief/nab3a-bundle": "^1.0",
        "react/eep": "dev-master",
        "react/http": "^0.4.1",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/monolog-bundle": "^3.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/symfony": "3.2.*",
        "yzalis/supervisor-bundle": "dev-develop"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:bangpound/DoctrineCouchDBBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:bangpound/couchdb-odm.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:bangpound/couchdb-client.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:R-Shief/nab3a-bundle.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:bangpound/SupervisorBundle.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:R-Shief/FOSJsRoutingBundle.git"
        }
    ],
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}

{
    "name": "rshief/kal3a",
    "license": "MIT",
    "type": "project",
    "description": "Aggregation and collection engine.",
    "autoload": {
        "psr-0": { "": "src/", "SymfonyStandard": "app/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bangpound/couchdb-client.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bangpound/DoctrineCouchDBBundle.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bangpound/elasticsearch-bundle-1.git"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/icu": "1.1.*@dev",
        "symfony/symfony": "2.5.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "jms/serializer-bundle": "dev-master",
        "yohang/dependency-tools": "1.0.*",
        "sonata-project/admin-bundle": "2.3.*@dev",
        "sonata-project/doctrine-orm-admin-bundle": "2.3.*@dev",
        "sonata-project/user-bundle": "~2.2",
        "doctrine/couchdb": "dev-develop as dev-master",
        "doctrine/couchdb-odm": "dev-master",
        "doctrine/couchdb-odm-bundle": "dev-develop",
        "robotsnowfall/enum": "dev-master",
        "mjohnson/decoda": "~6.2.3",
        "ddeboer/data-import-bundle": "dev-master",
        "ddeboer/data-import": "dev-master",
        "liip/monitor-bundle": "~2.0.2",
        "ushios/elasticsearch-bundle": "dev-develop",
        "bangpound/atom-data-bundle": "dev-master",
        "fennb/phirehose": "dev-master",
        "bangpound/twitter-streaming-bundle": "dev-master",
        "bangpound/pubsub-bundle": "dev-master",
        "bangpound/castle-bundle": "dev-master",
        "creof/doctrine2-spatial": "dev-master",
        "phayes/geophp": "dev-master",
        "sensiolabs/security-checker": "~1.2",
        "nojimage/twitter-text-php": "dev-master",
        "bangpound/guzzle-proxy-bundle": "dev-master",
        "dr-que/polynomial-regression": "dev-master",
        "bangpound/doctrine-datetime": "dev-master",
        "cypresslab/angular-ng-routing-bundle": "~1.0@dev",
        "nesbot/Carbon": "~1.8",
        "friendsofsymfony/rest-bundle": "1.3.*",
        "oldsound/rabbitmq-bundle": "~1.5.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "raulfraile/ladybug-bundle": "~1.0.2"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Yohang\\DependencyTools::installDeps",
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Liip\\MonitorBundle\\Composer\\ScriptHandler::checkHealth"
        ],
        "post-update-cmd": [
            "Yohang\\DependencyTools::installDeps",
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Liip\\MonitorBundle\\Composer\\ScriptHandler::checkHealth"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.5-dev"
        },
        "dependency-tools": {
            "bower": true
        }
    }
}

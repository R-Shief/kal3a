{
    "name": "rshief/kal3a",
    "license": "MIT",
    "type": "project",
    "description": "Aggregation and collection engine.",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "bangpound/doctrine-datetime": "~1.0@dev",
        "bangpound/guzzle-proxy-bundle": "~2.0@dev",
        "caxy/elasticsearch-bundle": "dev-develop",
        "creof/doctrine2-spatial": "dev-master",
        "doctrine/couchdb": "dev-master",
        "doctrine/couchdb-odm": "~1.0@alpha",
        "doctrine/couchdb-odm-bundle": "dev-master",
        "doctrine/doctrine-cache-bundle": "~1.0",
        "friendsofsymfony/jsrouting-bundle": "~2.0@dev",
        "friendsofsymfony/rest-bundle": "~1.5",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "javiereguiluz/easyadmin-bundle": "~1.4",
        "liip/monitor-bundle": "~2.3@dev",
        "nelmio/cors-bundle": "~1.0",
        "nesbot/Carbon": "~1.12",
        "nojimage/twitter-text-php": "dev-master",
        "oldsound/rabbitmq-bundle": "~1.6.0@dev",
        "phayes/geophp": "dev-master",
        "wikimedia/composer-merge-plugin": "^1.3",
        "nelmio/api-doc-bundle": "^2.11"
    },
    "require-dev": {
        "sensio/generator-bundle": "~3.0",
        "symfony/phpunit-bridge": "~2.7"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "Liip\\MonitorBundle\\Composer\\ScriptHandler::checkHealth"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "Liip\\MonitorBundle\\Composer\\ScriptHandler::checkHealth"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "symlink",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "merge-plugin": {
            "include": [
                "bundles/*/composer.json"
            ]
        }
    }
}

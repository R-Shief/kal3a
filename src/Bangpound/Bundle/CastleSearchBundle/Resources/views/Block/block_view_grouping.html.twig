{% extends sonata_block.templates.block_base %}

{% block block %}
    <table class="table table-condensed">
    {% for grouping,values in results %}
        {% for key,value in values %}
            <tr>
                <td>{{ grouping }}</td>
                <td>{{ key }}</td>
                <td>{{ value }}</td>
            </tr>
        {% endfor %}
    {% else %}
            No feeds available.
    {% endfor %}
    </table>
{% endblock %}

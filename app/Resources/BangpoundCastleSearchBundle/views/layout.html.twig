{% extends '::base.html.twig' %}

{% block stylesheets %}
    <link href="{{ asset('bundles/rshiefkal3a/css/style.css') }}" type="text/css" rel="stylesheet" />
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700" type="text/css" rel="stylesheet" />
    <link href="http://fonts.googleapis.com/css?family=Roboto+Condensed:400,300,700" type="text/css" rel="stylesheet" />
    <link href="//netdna.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.css" type="text/css" rel="stylesheet" />
    <style>
        [ng\:cloak], [ng-cloak], [data-ng-cloak], [x-ng-cloak], .ng-cloak, .x-ng-cloak {
            display: none !important;
        }
    </style>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript" src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
    <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
    <script>
        var castle = {
            typeNames: {{ castle_type_names | json_encode | raw }}
        };
    </script>
{% endblock %}

{% block body %}
    <div ng-app="castleSearch">
        <div{{ block('body_controller') }}>
